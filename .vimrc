let &termencoding=&encoding
set fileencodings=utf-8,gb18030,gbk,gb2312,big5
set nu
set nocompatible
syntax on
set hlsearch
let Tlist_Ctags_Cmd="/usr/local/bin/ctags"
nmap <F8> <ESC>:SrcExplToggle<CR> 
nmap <F9> <ESC>:Tlist<CR> 
nmap <F10> <ESC>:copen<CR>
nmap <C-H> <C-W>h
nmap <C-J> <C-W>j
nmap <C-K> <C-W>k
nmap <C-L> <C-W>l 

